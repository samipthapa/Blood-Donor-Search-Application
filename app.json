          "apiKey": "AIzaSyDXeYZmH905YwpneNWy8b27b2FqVYJ8FiA"
        }
      },
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.samipbthapa.BloodDonorSearch"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c8de2556-cf7f-46ec-b404-83761cab6d3b"
      }
    },
    "plugins": [
      "@react-native-firebase/app"
